{
    "name": "vikkio88/nicu-framework",
    "description": "a small framework (built on slim + php-di) to create restful api",
    "keywords": ["slim", "php-di", "mvp", "rest", "microframework", "microservice"],
    "license": "MIT",
    "authors": [
        {
            "name": "Vincenzo Ciaccio",
            "email": "vincenzo.ciaccio@gmail.com"
        }
    ],
    "require": {
        "hassankhan/config": "^0.11.2",
        "php-di/slim-bridge": "^2.0",
        "vlucas/phpdotenv": "^2.4",
        "tuupola/cors-middleware": "^0.5.2",
        "visavi/cleanup": "^1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "squizlabs/php_codesniffer": "^3.2",
        "phpmd/phpmd": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "Nicu\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nicu\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "build": [
            "php scripts/build.php"
        ]
    }
}
